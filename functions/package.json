    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "16"
  },
  "main": "lib/index.js",
  "dependencies": {
    "firebase-admin": "^8.0.0",
    "firebase-functions": "3",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "@types/request": "^2.48.2",
    "typescript": "^4.9.5"
  },
  "private": true
}
